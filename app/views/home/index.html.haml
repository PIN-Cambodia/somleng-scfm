- content_for :javascripts do
  = javascript_include_tag "map.js"

#map

.jumbotron.bg-dark
  .container
    / Example row of columns
    .row.text-center.ews-stats
      .col-md-4.ews-stats-item
        %h2.stat-head.text-grey Registered Users
        %h2.stat-highlight.text-light
          = number_with_delimiter(@contacts)
      .col-md-4.ews-stats-item
        %h2.stat-head.text-grey Sensors Active
        %h2.stat-highlight.text-light
          = @sensors
      .col-md-4.ews-stats-item
        %h2.stat-head.text-grey Alerts Sent
        %h2.stat-highlight.text-light
          = number_with_delimiter(@call_participation)



.container
